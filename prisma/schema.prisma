generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  Id           String    @id @default(uuid())
  userId       String    @unique
  access_token String
  username     String
  account_type String
  Keyword      KeyWord[]
}

model KeyWord {
  Id        String   @id @default(uuid())
  userId    String
  Key       String
  Value     String
  Variables String[]
  user      User     @relation(fields: [userId], references: [userId])
}
