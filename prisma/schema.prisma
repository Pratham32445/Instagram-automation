generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  Id           String    @id @default(uuid())
  userName     String    @unique
  accountId    String
  account_type String
  access_token String
  keyword      KeyWord[]
}

model KeyWord {
  Id       String @id @default(uuid())
  userName String @unique
  Key      String
  Value    String
  user     User   @relation(fields: [userName], references: [userName])
}
